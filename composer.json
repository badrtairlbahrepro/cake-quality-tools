{
    "name": "badrtairlbahrepro/cake-quality-tools",
    "type": "cakephp-plugin",
    "description": "Quality testing tools dashboard for CakePHP applications",
    "keywords": [
        "cakephp",
        "plugin",
        "quality",
        "testing",
        "phpunit",
        "phpstan",
        "codesniffer"
    ],
    "homepage": "https://github.com/badrtairlbahrepro/cake-quality-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Badr Tairlbahre",
            "email": "contact@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.0 || ^5.0",
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CakeQualityTools\\": "src/"
        }
    }
}

